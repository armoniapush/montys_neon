{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{ .Shell }} - {{ .Path }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "session",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#4A4A4A",
          "template": " \uE72A {{ .UserName }} ",
          "properties": {
            "display_host": false
          }
        },
        {
          "type": "os",
          "style": "powerline",
          "powerline_symbol": "\ue0b4",
          "foreground": "#ffffff",
          "background": "#326CE5",
          "template": " {{.Icon}} ",
          "properties": {
            "arch": "\uf303",
            "debian": "\uf306",
            "fedora": "\uf30a",
            "linux": "\uf17c",
            "macos": "\uf179",
            "manjaro": "\uf312",
            "opensuse": "\uf314",
            "ubuntu": "\uf31b",
            "windows": "\uf17a"
          }
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b4",
          "foreground": "#ffffff",
          "background": "#170B3B",
          "template": " \uF07B {{ .Path }} ",
          "properties": {
            "folder_icon": "\uF07B",
            "folder_separator_icon": "/",
            "home_icon": "~",
            "max_depth": 3,
            "style": "agnoster_full"
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\ue0b4",
          "foreground": "#2b06e6",
          "background": "#1e0366",
          "template": " \uE725 {{ .HEAD }}{{ if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uF044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uF046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uEAE7 {{ .StashCount }}{{ end }} ",
          "properties": {
            "branch_template": "{{ .Branch }}",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          }
        },
        {
          "type": "executiontime",
          "style": "powerline",
          "powerline_symbol": "\ue0b4",
          "foreground": "#EFDCF9",
          "background": "#f50742",
          "template": " \u23F1 {{ .FormattedMs }} ",
          "properties": {
            "always_enabled": true
          }
        },
        {
          "type": "status",
          "style": "diamond",
          "trailing_diamond": "\ue0b4",
          "template": " \uF08A{{ if gt .Code 0 }} {{.Code}}{{ end }} ",
          "foreground": "#EFDCF9",
          "background": "#2d1aae",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#FF3C3C{{ end }}"
          ]
        }
      ]
    },
    {
      "type": "rprompt",
      "alignment": "right",
      "segments": [
        {
          "type": "python",
          "style": "powerline",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#306998",
          "template": " \uE71B {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
          "properties": {
            "fetch_version": false
          }
        },
        {
          "type": "node",
          "style": "powerline",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#E63A29",
          "template": " \uE718{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "properties": {
            "fetch_version": false
          }
        },
        {
          "type": "battery",
          "style": "powerline",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0",
          "foreground": "#0E050F",
          "background_templates": [
            "{{if .Error}}#FF3C3C{{end}}",
            "{{if le .Percentage 20}}#FF3C3C{{end}}",
            "{{if and (gt .Percentage 20) (lt .Percentage 60)}}#E4F34A{{end}}",
            "{{if ge .Percentage 60}}#42E66C{{end}}"
          ],
          "template": " {{ .Icon }} {{ .Percentage }}% ",
          "properties": {
            "icons": [
              "\uf244",
              "\uf243",
              "\uf242",
              "\uf241",
              "\uf240"
            ]
          }
        },
        {
          "type": "weather",
          "style": "powerline",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0",
          "foreground": "#0E050F",
          "background": "#FFB86C",
          "template": " {{.Weather}} {{.Temperature}}{{.UnitIcon}} ",
          "properties": {
            "location": "Puerto la Cruz, VE",
            "units": "metric"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "time",
          "style": "powerline",
          "powerline_symbol": "\ue0b4",
          "foreground": "#EFDCF9",
          "background": "#220560",
          "template": " \uF017 {{ .CurrentDate | date .Format }} ",
          "properties": {
            "time_format": "15:04:05"
          }
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#EFDCF9",
          "template": "‚ùØ "
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
